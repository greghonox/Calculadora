            Button:
                text: 'Manuntencao'
            
        GridLayout:
            cols:1
            rows:1
            size_hint:1,1
            Image:
                source:"/home/greg/Imagens/logo.png"
        Label:
            size_hint:1,None
            height:'38dp'
            text:"status"
            
            