            orientation:"horizontal"
            pos_hint:{'top': 0.1}
            size_hint:1,None
            height:'38dp'
            ToggleButton:
                text: "Arquivo"
                
            Button:
                text: 'Editar'
                id: editar
                on_press: root.Editar()
            Button:
                text: 'Exibir'
